app.controller('JobController', [ 'JobServices', '$http', '$rootscope',
		'$location', '$scope',
		function(JobServices, $http, $rootscope, $location, $scope) {

			$scope.message = "Message from Job Controller"
			console.log("Starting-->JobController")

			var self = this;
			self.job = {
				id : '',
				title : '',
				qualification : '',
				description : '',
				status : '',
				date : ''
			}

		} ])